<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <Match>
    <Class name="org.wuerthner.cwn.api.CwnAccent" />
    <Bug pattern="MS_OOI_PKGPROTECT" />
  </Match>
  <Match>
    <Class name="org.wuerthner.cwn.api.CwnSymbolEvent" />
    <Bug pattern="MS_OOI_PKGPROTECT" />
  </Match>
  <Match>
    <Class name="org.wuerthner.cwn.api.CwnTrack" />
    <Bug pattern="MS_MUTABLE_ARRAY" />
  </Match>
  <Match>
    <Class name="org.wuerthner.cwn.api.ScoreParameter" />
    <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD" />
  </Match>
  <Match>
    <Class name="org.wuerthner.cwn.metric.Ratio" />
    <Or>
      <Bug pattern="EI_EXPOSE_REP2" />
      <Bug pattern="EI_EXPOSE_REP" />
    </Or>
  </Match>
  <Match>
    <Package name="org.wuerthner.cwn.midi" />
  </Match>
  <Match>
    <Package name="org.wuerthner.cwn.midi.event" />
  </Match>
  <Match>
    <Package name="org.wuerthner.cwn.midi.event.meta" />
  </Match>
  <Match>
    <Package name="org.wuerthner.cwn.midi.util" />
  </Match>
  <Match>
    <Class name="org.wuerthner.cwn.score.Score" />
    <Bug pattern="MS_MUTABLE_ARRAY" />
  </Match>

</FindBugsFilter>
